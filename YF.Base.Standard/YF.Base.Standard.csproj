<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>yu</Authors>
    <AssemblyVersion>1.0.0.2</AssemblyVersion>
    <FileVersion>1.0.0.2</FileVersion>
    <RootNamespace>YF.Base</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/kensen/YFLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kensen/YFLib</RepositoryUrl>
    <PackageTags>dapper repository extend</PackageTags>
    <PackageReleaseNotes>.net core 下的 dapper repository 扩展。</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\YF.Base\**\*.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.5" />
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
    <PackageReference Include="System.Reflection.Emit" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Dapper">
      <HintPath>..\packages\Dapper.1.50.2\lib\net45\Dapper.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
